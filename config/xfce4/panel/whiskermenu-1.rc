favorites=exo-web-browser.desktop,exo-mail-reader.desktop,exo-file-manager.desktop,pidgin.desktop,gmusicbrowser.desktop,ubuntu-software-center.desktop,exo-terminal-emulator.desktop,xfhelp4.desktop
recent=users.desktop,xfce-backdrop-settings.desktop,ubuntu-software-center.desktop,time.desktop,xfce-session-settings.desktop,thunar-volman-settings.desktop,systemback.desktop,exo-preferred-applications.desktop,xfce4-taskmanager.desktop,exo-terminal-emulator.desktop
